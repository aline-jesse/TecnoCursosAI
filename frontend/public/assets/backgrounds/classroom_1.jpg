  <rect x="280" y="200" width="40" height="30" fill="#F59E0B" stroke="#D97706" stroke-width="2"/>
  <rect x="330" y="200" width="40" height="30" fill="#F59E0B" stroke="#D97706" stroke-width="2"/>
  
