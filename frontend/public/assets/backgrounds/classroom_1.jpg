  <line x1="300" y1="100" x2="380" y2="100" stroke="#3B82F6" stroke-width="2"/>
  
  <!-- Nuvens na janela -->
  <circle cx="325" cy="80" r="8" fill="#FFFFFF" opacity="0.8"/>
  <circle cx="335" cy="85" r="6" fill="#FFFFFF" opacity="0.8"/>
  <circle cx="355" cy="75" r="7" fill="#FFFFFF" opacity="0.8"/>
  
